{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "eslint-config-prettier"
  ],
  "rules": {
    "semi": 2,
    "no-unused-vars": 0,
    "no-console": [1, {
      "allow": ["warn", "error", "info"]
    }],
    "no-prototype-builtins": 0,
    "@typescript-eslint/no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
    "curly": 2
  },
  "env": {
    "es6": true,
    "browser": true
  }
}